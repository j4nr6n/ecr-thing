{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": 0,
        "no-unused-vars": 0
    }
}
